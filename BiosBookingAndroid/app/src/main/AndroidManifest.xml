<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.proyectofinal.analistas.biospilayandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design">
        <activity android:name="com.proyectofinal.analistas.biospilayandroid.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.proyectofinal.analistas.biospilayandroid.Activities.ObraDetailActivity"
                    android:parentActivityName="com.proyectofinal.analistas.biospilayandroid.Activities.MainActivity"/>
        <activity android:name="com.proyectofinal.analistas.biospilayandroid.Activities.AddMaterialActivity"
            android:parentActivityName="com.proyectofinal.analistas.biospilayandroid.Activities.MaterialListActivity"/>
        <activity android:name="com.proyectofinal.analistas.biospilayandroid.Activities.MaterialListActivity"
            android:parentActivityName="com.proyectofinal.analistas.biospilayandroid.Activities.ObraDetailActivity"/>
        <activity android:name="com.proyectofinal.analistas.biospilayandroid.Activities.MovimientosActivity"
            android:parentActivityName="com.proyectofinal.analistas.biospilayandroid.Activities.MaterialListActivity"/>
        <activity android:name="com.proyectofinal.analistas.biospilayandroid.Activities.MaterialInformationActivity"
            android:parentActivityName="com.proyectofinal.analistas.biospilayandroid.Activities.MaterialListActivity"/>
    </application>

</manifest>